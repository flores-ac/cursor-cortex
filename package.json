{
  "name": "cursor-cortex",
  "version": "1.1.0",
  "description": "Knowledge management system integrated with commit tracking",
  "main": "index.js",
  "type": "module",
  "bin": {
    "cursor-cortex": "./cursor-cortex-cli.js"
  },
  "scripts": {
    "start": "node index.js",
    "cli": "node cursor-cortex-cli.js",
    "test": "node test/test.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "vector:demo": "node demo-semantic-search.js",
    "vector:test": "node test-tensorflow.js",
    "docker:build": "./scripts/docker-build.sh",
    "docker:start": "docker-compose up -d",
    "docker:stop": "docker-compose down",
    "docker:test": "./scripts/test-vector-search.sh",
    "docker:logs": "docker-compose logs -f cursor-cortex",
    "embeddings:generate": "docker-compose run --rm embeddings-generator"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.11.0",
    "@tensorflow/tfjs": "^4.4.0",
    "@tensorflow/tfjs-backend-cpu": "^4.4.0",
    "@tensorflow/tfjs-core": "^4.4.0",
    "@tensorflow/tfjs-converter": "^4.4.0",
    "@tensorflow-models/universal-sentence-encoder": "^1.3.3",
    "@tensorflow/tfjs-node": "^4.4.0",
    "chalk": "^5.2.0",
    "commander": "^9.4.1"
  },
  "keywords": [
    "ai-memory",
    "cursor-extension", 
    "semantic-search",
    "knowledge-management",
    "tacit-knowledge",
    "team-collaboration",
    "background-agents",
    "commits",
    "branch-notes",
    "jira"
  ],
  "cursor": {
    "mcp": {
      "command": "node index.js",
      "displayName": "Cursor-Cortex",
      "description": "AI memory system that transforms Cursor into an experienced teammate with semantic search, knowledge archaeology, and team collaboration",
      "debug": true
    }
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "eslint": "^9.26.0"
  }
}
